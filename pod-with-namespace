apiVersion: v1
kind: Namespace
metadata:
  name: my-custom-namespace
---
apiVersion: v1
kind: Pod
metadata:
  name: my-nginx-pod
  namespace: my-custom-namespace # This specifies the namespace for the pod
spec:
  containers:
  - name: nginx-container
    image: nginx:latest
    ports:
    - containerPort: 80
